<h1>New account</h1>

<% form_for(@account) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :full_name %><br />
    <%= f.text_field :full_name %>
  </p>
  <p>
    <%= f.label :admin_name %><br />
    <%= f.text_field :admin_name %>
  </p>
  <p>
    <%= f.label :admin_phone %><br />
    <%= f.text_field :admin_phone %>
  </p>
  <p>
    <%= f.label :admin_email %><br />
    <%= f.text_field :admin_email %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', accounts_path %>
